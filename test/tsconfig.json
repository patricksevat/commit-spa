{
  "compilerOptions": {
    "outDir": "./.tsbuild/",
    "module": "commonjs",
    "skipLibCheck": true,
    "removeComments": true,
    "noImplicitAny": true,
    "strictPropertyInitialization": true,
    "strictNullChecks": true,
    "types": ["node", "webdriverio/async", "jasmine", "@wdio/jasmine-framework"],
    "target": "es2019"
  },
  "include": [
    "pages/**/*.ts",
    "specs/**/*.ts",
    "../node_modules/webdriverio/async.d.ts"
  ]
}
